import ReactDOM from "react-dom";
import React from 'react';

const user = {
    firstName: "???",
    phoneNumber: 12345
}

function sayTheName(user) {
    return user.firstName;
}

function sayPhoneNumber() {
    return user.phoneNumber;
}

const greeting = (
    <h1>
        Hello, {sayTheName(user)}
    </h1>
)

const tellPhone = (
    <p>
        Your phone number is : {sayPhoneNumber()}
    </p>
)

ReactDOM.render (
    greeting,
    tellPhone,
    document.getElementById("root")
)
